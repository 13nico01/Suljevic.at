{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "default",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            {
                "name": "git",
                "icon": "logo-github",
                "autoExecuteCommands": true,
                "commands": [
                    "git status"
                ]
            },
            {
                "name": "install",
                "icon": "cloud-download",
                "commands": [
                    "npm install"
                ]
            },
            {
                "name": "start",
                "icon": "code",
                "commands": [
                    ""
                ]
            }
        ],
        "saved-session": []
    }
}